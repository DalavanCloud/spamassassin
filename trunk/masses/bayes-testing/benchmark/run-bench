#!/bin/bash

TESTNAME=$1
RESULTNAME=$2
PROGNAME=`which $0`
DIRNAME=`dirname $PROGNAME`

RESULTSPATH=$DIRNAME/results/$RESULTNAME
TESTSPATH=$DIRNAME/tests/$TESTNAME

mkdir -p $RESULTSPATH

cp -R $TESTSPATH/* $RESULTSPATH

$DIRNAME/run-bench.driver $TESTNAME $RESULTSPATH > $RESULTSPATH/output.txt 2>&1


