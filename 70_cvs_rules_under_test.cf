header T_USER_AGENT_UBB      X-Mailer =~ /^UBB\.?[Tt]hreads$/
score  T_USER_AGENT_UBB      -0.5
tflags T_USER_AGENT_UBB      nice

header T_USER_AGENT_XIMIAN   X-Mailer =~ /^Ximian Evolution \d+\.\d+\.\d+(?:\.\d+)?\s$/
