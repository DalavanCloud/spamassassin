header T_SUBJECT_SEXUAL_8		Subject =~ /[s5][e3\xE8-\xEB]x[u\xB5\xF9-\xFC][a4\xE0-\xE6@][l!|1](?:[l!|1]y)?.{0,4}[e3\xE8-\xEB]xp[l!|1][i1!|l\xEC-\xEF][ct][i1!|lE\xEC-\xEF][ct]/i

# short message-ids
header T_MSGID_SHORT_4	MESSAGEID =~ /^.{1,4}$/
header T_MSGID_SHORT_5	MESSAGEID =~ /^.{1,5}$/
header T_MSGID_SHORT_6	MESSAGEID =~ /^.{1,6}$/
header T_MSGID_SHORT_7	MESSAGEID =~ /^.{1,7}$/
header T_MSGID_SHORT_8	MESSAGEID =~ /^.{1,8}$/

# no To or Cc
header __CC_UNSET	Cc =~ /^UNSET$/ [if-unset: UNSET]
header __TO_UNSET	To =~ /^UNSET$/ [if-unset: UNSET]
header __CC_EXISTS	exists:Cc
header __TO_EXISTS	exists:To
meta T_TO_CC_UNSET	__CC_UNSET && __TO_UNSET 
meta T_TO_CC_NONE	!__CC_EXISTS && !__TO_EXISTS
header T_TOCC_UNSET	ToCc =~ /^UNSET$/ [if-unset: UNSET]
header __TOCC_EXISTS	exists:ToCc
meta T_TOCC_NONE	!__TOCC_EXISTS

