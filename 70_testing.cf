# let's see how this works
meta T_SPF_PASS_NO_SBL		(SPF_PASS && !RCVD_IN_SBL)
meta T_SPF_HELO_PASS_NO_SBL	(SPF_HELO_PASS && !RCVD_IN_SBL)
