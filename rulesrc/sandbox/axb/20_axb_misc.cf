# 2010-12-29
header          __UA_MOZ5               User-Agent =~ /^Mozilla\/5/
header          __UA_NIX                User-Agent =~ /(?:Macintosh|X11)/
header          __UA_NT4                User-Agent =~ /U\; NT4\.0\;/
meta            AXB_TRACK_MOZ           (__UA_MOZ5 && __UA_NIX && __UA_NT4)
describe        AXB_TRACK_MOZ           MozTrack
#score           AXB_TRACK_MOZ           0.001


