# just a test...
body T_ORANGES_AND_SPAM_POST	/[a-z]{3,},? Fl(?:orida)? (?:33|32|34)\d{3}\b/i
body T_ORANGES_AND_SPAM_TELE	/\b1?\D{0,2}(?:239|305|321|352|386|407|561|727|754|772|786|813|850|863|904|941|954)\D{0,2}\d{3}\D{0,2}\d{4}\b/
meta T_ORANGES_AND_SPAM_1	T_ORANGES_AND_SPAM_POST && T_ORANGES_AND_SPAM_TELE
meta T_ORANGES_AND_SPAM_2	T_ORANGES_AND_SPAM_POST + T_ORANGES_AND_SPAM_TELE == 1
meta T_ORANGES_AND_SPAM_3	T_ORANGES_AND_SPAM_POST && !T_ORANGES_AND_SPAM_TELE
meta T_ORANGES_AND_SPAM_4	!T_ORANGES_AND_SPAM_POST && T_ORANGES_AND_SPAM_TELE

